<%# add navigation links to this file %>
<% if user_signed_in? %> 
	<li class="dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Places<span class="caret"></span></a>
				<ul class="dropdown-menu">
						<li><a href="/places">All Visited Places</a></li>
								<% current_user.places.each do |place|%>
									<li><a href="/places/<%= place.id%>" class="dropdown_nav"><%= place.name%></a></li>
								<%end%>
				</ul>
	</li>
	<li><%= link_to 'Profile', users_profile_path %></li>
	<li><%= link_to 'Search', search_path %></li>
	<li><%= link_to 'Sign Out', destroy_user_session_path, method: :delete %></li>
<% else %> 
	<li><%= link_to 'Sign In', new_user_session_path %></li>
	<li><%= link_to 'Sign Up', new_user_registration_path %></li>
<% end %>

